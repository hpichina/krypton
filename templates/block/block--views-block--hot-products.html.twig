<section class="product-lists">
    <div class="container page-block">
        <div class="head">
        <h3>
            <i class="icon-basket"></i>
            <span>{{ label }}</span>
        </h3>
        </div>
        <ul class="row image-list product-list">
            {% for row in content["view_build"]["#rows"][0]["#rows"] %}
                <li class="col-md-3 col-sm-6 col-xs-12">
                {{ row }}
                </li>
            {% endfor %}
        </ul>
        <div class="text-center">
            <a class="btn btn-primary btn-lg" href="/products/overview">查看更多</a>
        </div>
    </div>
</section>

